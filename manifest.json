
{

  "description": "My web extension",
  "manifest_version": 2,
  "name": "Study08-Screencapturing",
  "version": "1.0",
  
  "web_accessible_resources": [
	  "html2canvas/dist/html2canvas.js"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["content.js", "html2canvas/dist/html2canvas.js"]
    }
  ]


}
